{
  "name": "board-game-asset-pipeline-example",
  "version": "0.1.0",
  "description": "\"Example repo for https://github.com/monday-sun/board-game-asset-pipeline\"",
  "repository": "https://github.com/monday-sun/board-game-asset-pipeline-example.git",
  "author": "Monday Romelfanger <monday.romelfanger@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "update-asset-pipeline": "yarn upgrade board-game-asset-pipeline && yarn install",
    "asset-gen:clean": "rm -rf ./generated",
    "asset-gen": "yarn ts-node node_modules/board-game-asset-pipeline/src/asset-pipeline",
    "asset-gen:watch": "yarn ts-node node_modules/board-game-asset-pipeline/src/asset-pipeline --watch",
    "build": "tsc",
    "build:watch": "yarn build -w",
    "format": "prettier --write .",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "postinstall": "npm install --prefix node_modules/board-game-asset-pipeline"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/react": "^18.3.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "globals": "^15.2.0",
    "prettier": "^3.2.5",
    "typescript-eslint": "^7.8.0"
  },
  "dependencies": {
    "board-game-asset-pipeline": "git+https://github.com/monday-sun/board-game-asset-pipeline#main",
    "react": "^18.3.1",
    "typescript": "^5.4.5"
  }
}
